<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yyyu.ssh.domain.SysPermissions" table="sys_permissions" schema="bos" dynamic-update="true">
        <id name="perId" column="per_id"/>
        <property name="perPid" column="per_pid"/>
        <property name="name" column="name"/>
        <property name="description" column="description"/>
        <property name="code" column="code"/>
        <property name="page" column="page"/>
        <property name="available" column="available"/>
        <property name="type" column="type"/>
    </class>
</hibernate-mapping>